<h2>Organizations</h2>
<div class="organizations row">
  {{#each this}}
  <div class="col-sm-3">
    <h3>{{name}}</h3>
    <p>{{description}}</p>
    <strong>Website: </strong> <a href="{{website}}" target="_blank">{{website}}</a><br/>
    <strong>Tags: </strong>
    {{#each tags}}
      [{{this}}]
    {{/each}}
  </div>
  {{/each}}
</div>
